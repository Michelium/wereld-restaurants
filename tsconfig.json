{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "ES2015",
        "lib": [
            "DOM",
            "ES2015"
        ],
        "downlevelIteration": true,
        "module": "commonjs",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "typeRoots": [
            "./node_modules/@types",
            "./assets/types"
        ]
    },
    "include": [
        "assets/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
